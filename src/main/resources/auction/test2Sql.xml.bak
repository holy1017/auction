<mapper namespace="test2">
	<select id="test2Select" parameterType="auction.test2.test2VO"
		resultType="auction.test2.test2VO">
		SELECT
		SYSDATE text
		, ROUND(DBMS_RANDOM.VALUE(1, 45),0) num
		,'asdsaga' text1
		, ROUND(DBMS_RANDOM.VALUE(1, 45),0) num1
		,'bgfhehert' text2
		, ROUND(DBMS_RANDOM.VALUE(1, 45),0) num2
		FROM dual
	</select>
	
